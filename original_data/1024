d><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">\n<strong>25</strong>\n</td><td rowspan="1" colspan="1">\n<strong>26.7</strong>\n</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1"></td></tr><tr><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">\n<strong>70.6</strong>\n</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1"></td></tr><tr><td rowspan="1" colspan="1">HIV1-1.3a</td><td rowspan="1" colspan="1">37</td><td rowspan="1" colspan="1">1</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">1/1</td></tr><tr><td rowspan="1" colspan="1">LRSVbeta</td><td rowspan="1" colspan="1">221</td><td rowspan="1" colspan="1">1</td><td rowspan="1" colspan="1">\n<strong>85.3</strong>\n</td><td rowspan="1" colspan="1">\n<strong>87.9</strong>\n</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">14.7</td><td rowspan="1" colspan="1">62.5</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/0</td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">*</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/0</td></tr><tr><td rowspan="1" colspan="1">TYMV</td><td rowspan="1" colspan="1">85</td><td rowspan="1" colspan="1">2</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">1/2</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/0</td></tr><tr><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">62.5</td><td rowspan="1" colspan="1">55.6</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">100</td><td rowspan="1" colspan="1">80</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">62.5</td><td rowspan="1" colspan="1">50</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>88.9</strong>\n</td><td rowspan="1" colspan="1"></td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1"></td></tr><tr><td rowspan="1" colspan="1">BWYV</td><td rowspan="1" colspan="1">69</td><td rowspan="1" colspan="1">1</td><td rowspan="1" colspan="1">50</td><td rowspan="1" colspan="1">57.1</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">37.5</td><td rowspan="1" colspan="1">50</td><td rowspan="1" colspan="1">1/1</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/1</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/0</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>88.9</strong>\n</td><td rowspan="1" colspan="1">1/1</td></tr><tr><td rowspan="1" colspan="1">JEV</td><td rowspan="1" colspan="1">138</td><td rowspan="1" colspan="1">1</td><td rowspan="1" colspan="1">100</td><td rowspan="1" colspan="1">78.3</td><td rowspan="1" colspan="1">1/3</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>90</strong>\n</td><td rowspan="1" colspan="1">1/2</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/1</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/0</td></tr><tr><td rowspan="1" colspan="1">MEV</td><td rowspan="1" colspan="1">138</td><td rowspan="1" colspan="1">1</td><td rowspan="1" colspan="1">\n<strong>100</strong>\n</td><td rowspan="1" colspan="1">\n<strong>72</strong>\n</td><td rowspan="1" colspan="1">1/2</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/1</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0</td><td rowspan="1" colspan="1">0/1</td><t