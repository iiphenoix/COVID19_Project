b'<?xml version="1.0" encoding="utf-8"?>\n<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">\n<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">\n    \n    <head><meta http-equiv="Content-Type" content="text/html; charset=utf-8" />\n        <!-- AppResources meta begin -->\n        <script type="text/javascript">var ncbi_startTime = new Date();</script>\n        <!-- AppResources meta end -->\n        \n        <!-- TemplateResources meta begin -->\n        <meta name="paf_template" content="" />\n\n        <!-- TemplateResources meta end -->\n        \n        <!-- Logger begin -->\n        <meta name="ncbi_db" content="pmc" /><meta name="ncbi_pdid" content="article" /><meta name="ncbi_acc" content="" /><meta name="ncbi_domain" content="irv" /><meta name="ncbi_report" content="record" /><meta name="ncbi_type" content="fulltext" /><meta name="ncbi_objectid" content="" /><meta name="ncbi_pcid" content="/articles/PMC2749972/" /><meta name="ncbi_app" content="pmc" />\n        <!-- Logger end -->\n        \n        <title>Ducks: The \xe2\x80\x9cTrojan Horses\xe2\x80\x9d of H5N1 influenza</title>\n        \n        <!-- AppResources external_resources begin -->\n        <link rel="stylesheet" href="/core/jig/1.14.8/css/jig.min.css" /><script type="text/javascript" src="/core/jig/1.14.8/js/jig.min.js"></script>\n\n        <!-- AppResources external_resources end -->\n        \n        <!-- Page meta begin -->\n        <meta name="robots" content="INDEX,NOFOLLOW,NOARCHIVE" /><link rel="canonical" href="https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2749972/" /><link rel="schema.DC" href="http://purl.org/DC/elements/1.0/" /><meta name="citation_journal_title" content="Influenza and Other Respiratory Viruses" /><meta name="citation_title" content="Ducks: The \xe2\x80\x9cTrojan Horses\xe2\x80\x9d of H5N1 influenza" /><meta name="citation_authors" content="Jeong\xe2\x80\x90Ki Kim, Nicholas J.